{
  "name": "dh-in-vscode",
  "displayName": "Deephaven in VS Code",
  "description": "",
  "publisher": "bmingles",
  "repository": {
    "type": "git",
    "url": "https://github.com/bmingles/dh-in-vscode"
  },
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:dhfs",
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dh-in-vscode.connect",
        "title": "Deephaven: Connect Server"
      },
      {
        "command": "dh-in-vscode.runCode",
        "title": "Deephaven: Run",
        "icon": "$(run-all)"
      },
      {
        "command": "dh-in-vscode.runSelection",
        "title": "Deephaven: Run Selected",
        "icon": "$(run)"
      },
      {
        "command": "dh-in-vscode.selectConnection",
        "title": "Deephaven: Select Connection"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "dh-in-vscode.runCode",
          "group": "navigation",
          "when": "editorLangId == python && editorFocus"
        },
        {
          "command": "dh-in-vscode.runSelection",
          "group": "navigation",
          "when": "editorLangId == python && editorFocus && editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "package": "vsce package -o releases/",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.87.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "ws": "^8.16.0"
  }
}
